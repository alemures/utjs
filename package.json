{
  "name": "utjs",
  "description": "utjs is a high performance library with some common functions like sort, concatArrays, randomString, stringToNumber, etc.",
  "version": "4.0.0",
  "author": "Alejandro Santiago Nieto",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cover": "npm test -- --coverage",
    "lint": "eslint *.js lib/**/*.js test/**/*.js scripts/**/*.js benchmark/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "types": "tsc",
    "types:generate": "tsc -p tsconfig.types.json && node scripts/fix-declaration.js",
    "check": "npm test && npm run lint && npm run types",
    "doc": "jsdoc ./lib/ ./package.json ./README.md -d ./doc",
    "prepack": "npm run check && npm run types:generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alemures/utjs.git"
  }
}
